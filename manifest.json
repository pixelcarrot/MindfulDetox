{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Block websites and redirect them to other pages.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}