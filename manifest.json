{
  "manifest_version": 3,
  "name": "Mindful Detox",
  "version": "1.0",
  "description": "Block websites and redirect them to other pages.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "history",
    "declarativeNetRequest"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}